    public partial class FormDashboard : Form
    {
        public FormDashboard()
        {
            InitializeComponent();
        }

          private void resumeShortlistToolStripMenuItem_Click(object sender, EventArgs e)
    {
        FormResumeShortlist form = new FormResumeShortlist();
        form.Show();     // open the form
        this.Hide();     // hide dashboard (optional)
    }

    private void settingsToolStripMenuItem_Click_1(object sender, EventArgs e)
    {
        MessageBox.Show("Settings page will come soon.", "Info",
                        MessageBoxButtons.OK, MessageBoxIcon.Information);
    }

